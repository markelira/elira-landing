  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:firebase\" --names \"NEXT,FIREBASE\" --prefix-colors \"cyan,yellow\"",
    "dev:next": "next dev --turbopack",
    "dev:firebase": "firebase emulators:start --only functions,firestore,auth,storage",
    "dev:next-only": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "firebase:start": "firebase emulators:start",
    "firebase:ui": "firebase emulators:start --only firestore,auth,storage --ui",
    "create-admin": "node create-admin-emulator.js",
    "deploy": "npm run build && firebase deploy",
    "deploy:hosting": "npm run build && firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "init-db": "tsx scripts/init-firestore.ts",
    "test-db": "tsx scripts/test-firestore.ts",
    "deploy-rules": "firebase deploy --only firestore:rules"
